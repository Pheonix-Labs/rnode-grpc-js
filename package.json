    "stat-mode": "^0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "grpc-tools": "^1.8.0",
    "npm-run-all": "^4.1.5",
    "protobufjs": "^6.8.8",
